  <postBuildCommand alwaysRun="False">cmd /C webpack --config webpack.config.js bin/test.js bin/index.js</postBuildCommand>
  <!-- Other project options -->
  <options>
    <option showHiddenPaths="False" />
    <option testMovie="Custom" />
